components:
  schemas:
    Customer:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        description:
          type: string
          nullable: true
          maxLength: 255
        first_name:
          type: string
          nullable: true
          maxLength: 255
        last_name:
          type: string
          nullable: true
          maxLength: 255
        job_title:
          type: string
          nullable: true
          maxLength: 255
        email:
          type: string
          format: email
          nullable: true
          maxLength: 255
        phone:
          type: string
          nullable: true
          maxLength: 255
        photo:
          type: string
          readOnly: true
        department:
          type: integer
          writeOnly: true
          nullable: true
        department_name:
          type: string
          readOnly: true
        organisation:
          type: integer
          writeOnly: true
          nullable: true
        organisation_name:
          type: string
          readOnly: true
      required:
      - department_name
      - id
      - organisation_name
      - photo
    Department:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 255
      required:
      - id
      - name
    Employee:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        job_title:
          type: string
          nullable: true
          maxLength: 255
        phone:
          type: string
          nullable: true
          maxLength: 255
        username:
          type: string
        first_name:
          type: string
        last_name:
          type: string
        email:
          type: string
          format: email
        password:
          type: string
          writeOnly: true
        photo:
          type: string
          readOnly: true
        department:
          type: integer
          writeOnly: true
          nullable: true
        department_name:
          type: string
          readOnly: true
      required:
      - department_name
      - email
      - first_name
      - id
      - last_name
      - password
      - photo
      - username
    Organisation:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 255
      required:
      - id
      - name
    PatchedCustomer:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        description:
          type: string
          nullable: true
          maxLength: 255
