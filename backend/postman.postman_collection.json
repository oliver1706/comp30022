          type: string
          nullable: true
          maxLength: 255
        last_name:
          type: string
          nullable: true
          maxLength: 255
        job_title:
          type: string
          nullable: true
          maxLength: 255
        email:
          type: string
          format: email
          nullable: true
          maxLength: 255
        phone:
          type: string
          nullable: true
          maxLength: 255
        photo:
          type: string
          readOnly: true
        department:
          type: integer
          writeOnly: true
          nullable: true
        department_name:
          type: string
          readOnly: true
        organisation:
          type: integer
          writeOnly: true
          nullable: true
        organisation_name:
          type: string
          readOnly: true
    PatchedDepartment:
      type: object
      properties:
        id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 255
    PatchedEmployee:
      type: object
